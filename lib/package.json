{
  "name": "lib",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "npm run start:dev",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "wait-and-test": "wait-on tcp:3000 --delay 60000 --interval 5000 -s && npm run jest",
    "create-test-env": "ts-node scripts/create-test-env.ts",
    "test": "concurrently \"npm run create-test-env\" \"npm run wait-and-test\"",
    "jest": "jest --config jest.config.js --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema=./src/prisma/schema.prisma",
    "prisma:update": "npm run prisma:pull && npm run prisma:generate"
  },
  "dependencies": {
    "@hedhog/utils": "^0.0.4",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.20.0",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "dotenv-stringify": "^3.0.1",
    "googleapis": "^144.0.0",
    "hcode-core": "^0.1.485",
    "mimemessage": "^1.0.5",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.7.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "prisma": "^5.20.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "wait-on": "^8.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@hedhog/file(|/.*)$": "<rootDir>/libs/file/src/$1",
      "^@hedhog/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@hedhog/mail(|/.*)$": "<rootDir>/libs/mail/src/$1",
      "^@hedhog/pagination(|/.*)$": "<rootDir>/libs/pagination/src/$1",
      "^@hedhog/setting(|/.*)$": "<rootDir>/libs/setting/src/$1",
      "^@hedhog/admin(|/.*)$": "<rootDir>/libs/admin/src/$1",
      "^@hedhog/person(|/.*)$": "<rootDir>/libs/person/src/$1"
    }
  }
}
