{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "file": {
      "type": "library",
      "root": "libs/file",
      "entryFile": "index",
      "sourceRoot": "libs/file/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file/tsconfig.lib.json"
      }
    },
    "prisma": {
