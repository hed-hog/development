{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@hedhog/auth": ["libs/admin/src/auth/src"],
      "@hedhog/auth/*": ["libs/admin/src/auth/src/*"],
      "@hedhog/file": ["libs/admin/src/file/src"],
      "@hedhog/file/*": ["libs/admin/src/file/src/*"],
      "@hedhog/prisma": ["libs/prisma/src"],
      "@hedhog/prisma/*": ["libs/prisma/src/*"],
      "@hedhog/mail": ["libs/admin/src/mail/src"],
      "@hedhog/mail/*": ["libs/admin/src/mail/src/*"],
      "@hedhog/user": ["libs/admin/src/user/src"],
      "@hedhog/user/*": ["libs/admin/src/user/src/*"],
      "@hedhog/pagination": ["libs/pagination/src"],
      "@hedhog/pagination/*": ["libs/pagination/src/*"],
      "@hedhog/permission": ["libs/admin/src/permission/src"],
      "@hedhog/permission/*": ["libs/admin/src/permission/src/*"],
      "@hedhog/screen": ["libs/admin/src/screen/src"],
      "@hedhog/screen/*": ["libs/admin/src/screen/src/*"],
      "@hedhog/menu": ["libs/admin/src/menu/src"],
      "@hedhog/menu/*": ["libs/admin/src/menu/src/*"],
      "@hedhog/admin": ["libs/admin/src"],
      "@hedhog/admin/*": ["libs/admin/src/*"]
    }
  }
}
