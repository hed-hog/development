{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@hadsys/hadsys-auth/(.*)": "<rootDir>/../libs/hadsys-auth/src/$1",
    "@hadsys/hadsys-auth": "<rootDir>/../libs/hadsys-auth/src",
    "@hadsys/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@hadsys/auth": "<rootDir>/../libs/auth/src",
    "@hadsy/file/(.*)": "<rootDir>/../libs/file/src/$1",
    "@hadsy/file": "<rootDir>/../libs/file/src",
    "@hadsys/file/(.*)": "<rootDir>/../libs/file/src/$1",
    "@hadsys/file": "<rootDir>/../libs/file/src",
    "@hadsys/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@hadsys/prisma": "<rootDir>/../libs/prisma/src",
    "@hadsys/mail/(.*)": "<rootDir>/../libs/mail/src/$1",
    "@hadsys/mail": "<rootDir>/../libs/mail/src"
  }
}