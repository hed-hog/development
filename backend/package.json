{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "HedHog",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "npm run start:dev",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{backend,admin}/**/*.ts\" --fix",
    "migrate:create": "typeorm migration:create ./src/typeorm/migrate",
    "migrate:down": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert -d ./src/typeorm/database.providers.ts && npm run prisma:pull && npm run prisma:generate",
    "migrate:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d ./src/typeorm/database.providers.ts && npm run prisma:pull && npm run prisma:generate",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema=./src/prisma/schema.prisma",
    "migrate": "ts-node ./scripts/migrate",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@hedhog/auth": "^0.0.25",
    "@hedhog/mail": "^0.0.13",
    "@hedhog/menu": "^0.0.11",
    "@hedhog/pagination": "^0.0.7",
    "@hedhog/permission": "^0.0.18",
    "@hedhog/prisma": "^0.0.7",
    "@hedhog/screen": "^0.0.4",
    "@hedhog/user": "^0.0.12",
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.19.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@hedhog/utils": "^0.0.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.1.3"
  }
}
